# .env.example (예시)

# Database Configuration
DATABASE_URL=jdbc:mariadb://db:3306/project-db # Docker 내부 DB 호스트 및 포트
DATABASE_USERNAME=root
DATABASE_PASSWORD=your_db_password_here # 실제 비밀번호 대신 설명

# JWT Configuration
JWT_SECRET=your_very_long_and_random_jwt_secret_key_here # 실제 비밀 키 대신 설명
JWT_EXPIRATION=3600 # 1 hour in seconds